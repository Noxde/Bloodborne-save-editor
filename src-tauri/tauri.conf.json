{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npx cross-env BROWSER=none npm start",
    "frontendDist": "../build",
    "devUrl": "http://localhost:3000"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": ["resources/*"],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "Bloodborne Save Editor",
  "mainBinaryName": "Bloodborne Save Editor",
  "version": "0.7.0",
  "identifier": "com.bloodborne.editor",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 700,
        "width": 1290,
        "minWidth": 1290,
        "minHeight": 700,
        "center": true,
        "resizable": true,
        "maximizable": true,
        "title": "Bloodborne Save Editor v0.7.0",
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
